{
  "name": "lil-dude",
  "version": "0.1.0",
  "description": "Your personal AI executive assistant",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "bin": {
    "lil-dude": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsup src/index.ts src/cli.ts --format esm --dts --clean",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "onboard": "tsx src/cli.ts onboard",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "web:dev": "cd web && vite dev",
    "web:build": "cd web && vite build"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/static": "^7.0.4",
    "@fastify/websocket": "^10.0.1",
    "better-sqlite3": "^11.0.0",
    "chalk": "^5.0.0",
    "commander": "^12.0.0",
    "discord.js": "^14.25.1",
    "fastify": "^4.29.1",
    "glob": "^10.0.0",
    "inquirer": "^9.3.8",
    "nanoid": "^5.0.0",
    "node-cron": "^3.0.3",
    "openai": "^4.104.0",
    "ora": "^8.0.0",
    "pino": "^9.0.0",
    "pino-pretty": "^11.0.0",
    "telegraf": "^4.16.3",
    "tiktoken": "^1.0.0",
    "ws": "^8.16.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.0",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^20.11.0",
    "@types/ws": "^8.18.1",
    "eslint": "^9.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.4.0",
    "typescript-eslint": "^8.0.0",
    "vitest": "^2.0.0"
  }
}
